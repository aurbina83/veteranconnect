    "ngApp/js/controllers/UserRegistrationController.ts",
    "ngApp/js/controllers/VerifiedController.ts",
    "ngApp/js/directives/nav.ts",
    "ngApp/js/filters/trunc_url.ts",
    "ngApp/js/filters/truncate.ts",
    "ngApp/js/services/AuthInterceptor.ts",
    "ngApp/js/services/CommentService.ts",
    "ngApp/js/services/ErrorService.ts",
    "ngApp/js/services/EventService.ts",
