{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "inlineSourceMap": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**",
    "!typings/main/**",
    "!typings/main.d.ts"
  ],
  "exclude": [
    "typings/main",
    "typings/main.d.ts"
  ],
  "compileOnSave": true,
  "buildOnSave": false,
  "files": [
